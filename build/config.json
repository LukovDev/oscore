{
    "config": {
        "device":            "atmega32u4",
        "port":              "COM5",
        "clock":             16000000,
        "serial-port-speed": 57600
    },

    "build-config": {
        "output-file-name": "oscore",
        "avrdude":          "/build/tools/avrdude/",
        "avr-gcc":          "/build/tools/avr-gcc/",
        "avr-gcc-flags": [
            "-ffunction-sections", "-fdata-sections",
            "-fpermissive", "-fno-exceptions",
            "-Wall", "-Wextra", "-Os", "-flto"
        ]
    }
}
